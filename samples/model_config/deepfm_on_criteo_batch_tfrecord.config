model_dir: "experiments/dac_deepfm"

train_config {
  log_step_count_steps: 20
  # fine_tune_checkpoint: ""
  optimizer_config: {
    adam_optimizer: {
      learning_rate: {
        exponential_decay_learning_rate {
          initial_learning_rate: 0.0001
          decay_steps: 10000
          decay_factor: 0.5
          min_learning_rate: 0.0000001
        }
      }
    }
    use_moving_average: false
  }

  num_steps: 100
}

eval_config {
  metrics_set: {
       auc {}
  }
}

