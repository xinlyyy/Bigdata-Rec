model_dir: "experiments/taobao_fg_ev_demo"

train_config {
  optimizer_config {
    use_moving_average: false
    adam_async_optimizer {
      learning_rate {
        exponential_decay_learning_rate {
          initial_learning_rate: 0.0001
          decay_steps: 100000
          decay_factor: 0.5
          min_learning_rate: 1e-07
        }
      }
    }
  }
  num_steps: 400
  sync_replicas: true
  log_step_count_steps: 200
}
eval_config {
  metrics_set {
    auc {
    }
  }
}
