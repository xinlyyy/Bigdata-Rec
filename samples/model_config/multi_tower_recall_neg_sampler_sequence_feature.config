train_config {
  optimizer_config: {
    adam_optimizer: {
      learning_rate: {
        exponential_decay_learning_rate {
          initial_learning_rate: 0.001
          decay_steps: 1000
          decay_factor: 0.5
          min_learning_rate: 1e-07
        }
      }
    }
    use_moving_average: false
  }
  num_steps: 6
  sync_replicas: false
  save_checkpoints_steps: 100
  log_step_count_steps: 2
}

