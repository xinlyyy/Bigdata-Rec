model_dir: "{TEST_DIR}"

train_config {
  optimizer_config: {
    adam_optimizer: {
      learning_rate: {
        exponential_decay_learning_rate {
          initial_learning_rate: 0.0001
          decay_steps: 100000
          decay_factor: 0.5
          min_learning_rate: 0.0000001
        }
      }
    }
    use_moving_average: false
  }
  log_step_count_steps: 200
  sync_replicas: true
}

eval_config {
  metrics_set: {
       auc {}
  }
}

datahub_train_input{
  akId:"{DH_ID}"
  akSecret:"{DH_KEY}"
  endpoint:"{DH_REG}"
  project:"{DH_PRO}"
  topic:"{DH_TOPIC}"
  offset_time: '20220627 09:22:00'
}

datahub_eval_input{
  akId:"{DH_ID}"
  akSecret:"{DH_KEY}"
  endpoint:"{DH_REG}"
  project:"{DH_PRO}"
  topic:"{DH_TOPIC}"
}

